{
  "version": 2,
  "buildCommand": "pnpm install && pnpm run build:frontend",
  "installCommand": "pnpm install",
  "framework": null,
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "https://3000-ien5pg0d6zwac274zk4rq-6ff05c9c.manus.computer/api/trpc/:path*"
    },
    {
      "source": "/api/stripe/webhook",
      "destination": "https://3000-ien5pg0d6zwac274zk4rq-6ff05c9c.manus.computer/api/stripe/webhook"
    },
    {
      "source": "/api/oauth/:path*",
      "destination": "https://3000-ien5pg0d6zwac274zk4rq-6ff05c9c.manus.computer/api/oauth/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

